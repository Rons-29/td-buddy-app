{
  "name": "qa-workbench",
  "displayName": "QA Workbench",
  "description": "QAエンジニアの専用作業台 - データ生成から品質管理まで、助手のブリューがサポート",
  "version": "0.1.0",
  "publisher": "qa-workbench",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Testing"
  ],
  "keywords": [
    "qa workbench",
    "data generation",
    "quality assurance",
    "test data",
    "brew assistant",
    "qa tools"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "qa-workbench.helloWorld",
        "title": "Hello from Brew",
        "category": "QA Workbench"
      },
      {
        "command": "qa-workbench.brewPassword",
        "title": "パスワード生成",
        "category": "QA Workbench",
        "icon": "$(key)"
      },
      {
        "command": "qa-workbench.brewPersonalInfo",
        "title": "個人情報生成",
        "category": "QA Workbench",
        "icon": "$(person)"
      }
    ],
    "keybindings": [
      {
        "command": "qa-workbench.brewPassword",
        "key": "ctrl+alt+p",
        "mac": "cmd+alt+p"
      }
    ]
  },
  "scripts": {
    "dev": "echo '🍺 Brew: QA Workbench開発サーバーを起動します...' && npm run dev:backend & npm run dev:frontend",
    "dev:backend": "cd td-buddy-webapp/backend && npm run dev",
    "dev:frontend": "cd td-buddy-webapp/frontend && npm run dev",
    "dev:backend-only": "cd td-buddy-webapp/backend && npm run dev",
    "dev:frontend-only": "cd td-buddy-webapp/frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd td-buddy-webapp/backend && npm run build",
    "build:frontend": "cd td-buddy-webapp/frontend && npm run build",
    "start": "npm run start:backend & npm run start:frontend",
    "start:backend": "cd td-buddy-webapp/backend && npm start",
    "start:frontend": "cd td-buddy-webapp/frontend && npm start",
    "install:all": "npm install && cd td-buddy-webapp/backend && npm install && cd ../frontend && npm install",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd td-buddy-webapp/backend && npm run lint",
    "lint:frontend": "cd td-buddy-webapp/frontend && npm run lint",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd td-buddy-webapp/backend && npm run test",
    "test:frontend": "cd td-buddy-webapp/frontend && npm run test",
    "type-check": "npm run type-check:backend && npm run type-check:frontend",
    "type-check:backend": "cd td-buddy-webapp/backend && npm run type-check",
    "type-check:frontend": "cd td-buddy-webapp/frontend && npm run type-check",
    "db:setup": "cd td-buddy-webapp/backend && npm run db:setup",
    "db:init": "cd td-buddy-webapp/backend && npm run db:init",
    "db:reset": "cd td-buddy-webapp/backend && npm run db:reset",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.0"
  },
  "dependencies": {}
}
